<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deyu.stupidgameone"
    android:versionCode="2"
    android:versionName="又稍微改良版" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.deyu.stupidgameone.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.deyu.stupidgameone.Game"
            android:label="@string/title_activity_game" >
        </activity>
        <activity
            android:name="com.deyu.stupidgameone.Finish"
            android:label="@string/title_activity_finish" >
        </activity>
        <activity android:name="com.facebook.LoginActivity"/>
        <activity
            android:name="com.deyu.stupidgameone.HighSroce"
            android:label="@string/title_activity_high_sroce" >
        </activity>
        <meta-data android:value="@string/app_id" android:name="com.facebook.sdk.ApplicationId"/>
    </application>

</manifest>
